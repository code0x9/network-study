{
  "name": "Simple Redis Container",
  "image": "redis:8.2-bookworm",

  "features": {
    "ghcr.io/devcontainers/features/docker-outside-of-docker:1": {}
  },

  "portsAttributes":{
    "6379": {
      "label": "Redis",
      "onAutoForward": "notify"
    }
  },

  "postCreateCommand": "redis-server --daemonize no"
}
